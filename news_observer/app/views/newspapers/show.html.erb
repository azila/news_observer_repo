<% @page_title = "Show Newspaper" %>

<%= link_to("<< Back to List", {:action => 'index'}, :class => 'back-link') %>

<div class="newspapers show">
  <h2>Show Newspaper</h2>

  <table summary="Newspaper detail view">
    <tr>
      <th>Name</th>
      <td><%= @newspaper.name %></td>
    </tr>
    <tr>
      <th>Position</th>
      <td><%= @newspaper.position %></td>
    </tr>
    <tr>
      <th>Visible?</th>
      <td><%= @newspaper.visible ? 'true' : 'false' %></td>
    </tr>
    <tr>
      <th>Created</th>
      <td><%= @newspaper.created_at %></td>
    </tr>
    <tr>
      <th>Updated</th>
      <td><%= @newspaper.updated_at %></td>
    </tr>
  </table>

</div>